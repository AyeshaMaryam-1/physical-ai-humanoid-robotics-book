#usda 1.0
(
    doc = "Camera link of the humanoid robot"
)

def "camera"
{
    def Cube "collision"
    {
        add xformOp:scale = (0.05, 0.05, 0.1)
        add xformOp:translate = (0, 0, 0)
        prepend apiSchemas = ["PhysicsRigidBodyAPI"]
    }

    def Cube "visual"
    {
        add xformOp:scale = (0.05, 0.05, 0.1)
        add xformOp:translate = (0, 0, 0)
        prepend apiSchemas = ["MaterialBindingAPI"]
    }
}